<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hsjiang.lifecycle">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LifeCycle">
        <activity
            android:name=".compose.receiver.BroadcastReceiverActivity"
            android:exported="true"
            android:label="@string/title_activity_brocast_receiver"
            android:theme="@style/Theme.LifeCycle" />
        <activity
            android:name=".compose.AndroidViewsInComposeActivity"
            android:exported="true"
            android:label="@string/title_activity_android_views_in_compose"
            android:theme="@style/Theme.LifeCycle" />
        <activity
            android:name=".origin.ComposeInAndroidViewsActivity"
            android:exported="true" />
        <activity
            android:name=".compose.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.LifeCycle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>